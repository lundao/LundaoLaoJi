  "name": "在论道你算老几",
  "description": "获取用户是论道第几个注册的",
  "version": "2.0",

  "content_scripts": [{
    "matches": ["*://*.lundao.com/*"],
    "js": ["js/jquery-2.1.4.min.js", "js/content_action.js"]
